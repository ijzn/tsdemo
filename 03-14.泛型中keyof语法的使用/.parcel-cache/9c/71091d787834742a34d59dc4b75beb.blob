class Teacher {
  constructor(info) {
    this.info = info;
  }

  getInfo(key) {
    return this.info[key];
  }

}

const teacher = new Teacher({
  name: 'dell',
  age: 18,
  gender: 'male'
});
const test = teacher.getInfo('hello');
console.log(test);